{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@typechain/*": [
        "../typechain-types/*"
      ],
      "typechain-types/*": [
        "../typechain-types/*"
      ]
    },
    "rootDirs": [
      ".",
      "../typechain-types"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "../typechain-types/**/*.ts"
  ]
}
